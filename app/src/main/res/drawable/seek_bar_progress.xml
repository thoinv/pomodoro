<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="line" android:innerRadius="3dp">
            <stroke android:width="@dimen/seek_bar_background_height" android:color="@color/seek_bar_background"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line" android:innerRadius="3dp">
                <stroke android:width="@dimen/seek_bar_progress_height" android:color="@color/seek_bar_progress"/>
            </shape>
        </clip>
    </item>
</layer-list>
